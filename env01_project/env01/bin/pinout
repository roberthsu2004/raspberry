#!/home/pi/Documents/raspberry/env01_project/env01/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'gpiozero==1.5.0','console_scripts','pinout'
__requires__ = 'gpiozero==1.5.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gpiozero==1.5.0', 'console_scripts', 'pinout')()
    )
