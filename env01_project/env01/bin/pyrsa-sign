#!/home/pi/Documents/raspberry/env01_project/env01/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'rsa==4.0','console_scripts','pyrsa-sign'
__requires__ = 'rsa==4.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rsa==4.0', 'console_scripts', 'pyrsa-sign')()
    )
